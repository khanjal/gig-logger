{
  "name": "raptor-gig",
  "version": "0.0.0",
  "browser": {
    "child_process": false,
    "fs": false,
    "process": false,
    "net": false,
    "tls": false
  },
  "files": [
    "./src",
    "./dist"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve:ssl": "ng serve --host gig-local.raptorsheets.com --ssl",
    "build": "ng build",
    "build:dev": "ng build --configuration development",
    "build:stage": "ng build --configuration staging",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "update-lambda": "node scripts/update-lambda.js",
    "setup-metrics": "node scripts/setup-metrics.js"
  },
  "private": true,
  "overrides": {
    "qs": "6.14.1"
  },
  "dependencies": {
    "@angular/animations": "^20.3.15",
    "@angular/cdk": "~20.2.14",
    "@angular/common": "^20.3.15",
    "@angular/compiler": "^20.3.15",
    "@angular/core": "^20.3.15",
    "@angular/forms": "^20.3.15",
    "@angular/material": "~20.2.14",
    "@angular/platform-browser": "^20.3.15",
    "@angular/platform-browser-dynamic": "^20.3.15",
    "@angular/router": "^20.3.15",
    "@angular/service-worker": "^20.3.15",
    "@auth0/angular-jwt": "^5.2.0",
    "angular-oauth2-oidc": "^20.0.2",
    "aws-amplify": "^6.15.7",
    "chart.js": "^4.5.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "dexie": "^4.2.1",
    "ng2-charts": "^8.0.0",
    "ngx-mat-timepicker": "^20.0.1",
    "rxjs": "^7.8.2",
    "uuid": "^11.0.5",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.7",
    "@angular/cli": "^20.3.7",
    "@angular/compiler-cli": "^20.3.15",
    "@types/jasmine": "^5.1.12",
    "@types/jest": "^30.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.9.2",
    "autoprefixer": "^10.4.22",
    "buffer": "^6.0.3",
    "jasmine-core": "^5.12.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "postcss": "^8.5.6",
    "process": "^0.11.10",
    "tailwindcss": "^3.4.18",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.5.4"
  }
}
