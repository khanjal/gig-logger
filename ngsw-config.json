{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.json",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**/*"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "google-fonts-stylesheets",
      "urls": [
        "https://fonts.googleapis.com/icon?family=Material+Icons"
      ],
      "cacheConfig": {
        "maxSize": 5,
        "maxAge": "1d",
        "strategy": "freshness"
      }
    },
    {
      "name": "google-fonts-webfonts",
      "urls": [
        "https://fonts.gstatic.com/s/materialicons/*"
      ],
      "cacheConfig": {
        "maxSize": 10,
        "maxAge": "30d",
        "strategy": "performance"
      }
    }
  ]
}