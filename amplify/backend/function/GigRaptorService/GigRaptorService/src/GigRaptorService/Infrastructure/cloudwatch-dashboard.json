          [ ".", "API.sheets-all.Success" ],
          [ ".", "User.DataSync" ],
          [ ".", "User.PlacesAutocomplete" ],
          [ ".", "User.TokenRefresh" ],
          [ ".", "User.TotalActivity" ]
        ],
        "view": "timeSeries",
        "stacked": false,
        "region": "us-east-1",
        "title": "Request Counts",
        "period": 300,
        "yAxis": {
          "left": {
            "label": "Count",
            "min": 0
          }
        }
      }
    },
    {
      "type": "metric",
      "x": 12,
      "y": 0,
      "width": 12,
      "height": 6,
      "properties": {
        "metrics": [
          [ "RaptorGig/Lambda", "API.sheets-all.Duration" ],
