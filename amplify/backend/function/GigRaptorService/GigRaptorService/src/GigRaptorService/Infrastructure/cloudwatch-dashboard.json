          [ ".", "Error.GeneralError" ],
          [ ".", "API.sheets-health.Error" ],
          [ ".", "API.sheets-all.Error" ],
          [ "AWS/Lambda", "Errors", "FunctionName", "raptor-gig-service" ]
        ],
        "view": "timeSeries",
        "stacked": false,
        "region": "us-east-1",
        "title": "Error Counts",
        "period": 300,
        "yAxis": {
          "left": {
            "label": "Errors",
            "min": 0
          }
        }
      }
    },
    {
      "type": "metric",
      "x": 12,
      "y": 6,
      "width": 12,
      "height": 6,
      "properties": {
        "metrics": [
          [ "RaptorGig/Lambda", "Sheets.SaveData.ItemCount" ],
          [ ".", "Places.Autocomplete.QueryLength" ],
          [ ".", "RateLimit.Hit" ],
          [ "AWS/Lambda", "Invocations", "FunctionName", "raptor-gig-service" ]
        ],
        "view": "timeSeries",
        "stacked": false,
        "region": "us-east-1",
        "title": "Usage Metrics",
        "period": 300,
        "yAxis": {
          "left": {
            "label": "Count/Length",
            "min": 0
          }
