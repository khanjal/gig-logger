          [ ".", "Sheets.SaveData.Duration" ],
          [ "AWS/Lambda", "Duration", "FunctionName", "raptor-gig-service" ]
        ],
        "view": "timeSeries",
        "stacked": false,
        "region": "us-east-1",
        "title": "Response Times (ms)",
        "period": 300,
        "yAxis": {
          "left": {
            "label": "Milliseconds",
            "min": 0
          }
        }
      }
    },
    {
      "type": "metric",
      "x": 0,
      "y": 6,
      "width": 12,
      "height": 6,
      "properties": {
        "metrics": [
          [ "RaptorGig/Lambda", "Error.Total" ],
